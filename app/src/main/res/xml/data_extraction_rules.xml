<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>

        <include domain="sharedpref" path="."/>
        <include domain="database" path="todo_database"/>
    </cloud-backup>
    <device-transfer>

        <include domain="sharedpref" path="."/>
        <include domain="database" path="todo_database"/>

        <exclude domain="file"/>
    </device-transfer>
</data-extraction-rules>